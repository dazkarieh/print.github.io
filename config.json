{
    "baseURL": "https://tin6.com",
    "languageCode": "zh-cn",
    "MetaDataFormat":"json",
    "title": "消夏錄",
    "theme": "hiruko",
    "markup": {
      "goldmark": {
         "extensions": {
            "definitionList": true,
            "footnote": true,
            "linkify": true,
            "enableEmoji": true,
            "strikethrough": true,
            "table": true,
            "taskList": true,
            "typographer": true
         },
         "parser": {
            "attribute": true,
            "autoHeadingID": true,
            "autoHeadingIDType": "github"
         },
         "renderer": {
            "hardWraps": true,
            "unsafe": true,
            "xHTML": false
         }
      },
      "highlight": {
         "codeFences": false,
         "guessSyntax": false,
         "hl_Lines": "",
         "lineNoStart": 1,
         "lineNos": false,
         "lineNumbersInTable": false,
         "noClasses": false,
         "style": "monokai",
         "tabWidth": 4
      }
    },
    "sitemap": {
      "changefreq": "weekly",
      "filename": "sitemap.xml",
      "priority": 0.5
    },
    "minify": {
      "disableCSS": false,
      "disableHTML": false,
      "disableJS": false,
      "disableJSON": false,
      "disableSVG": false,
      "disableXML": false,
      "minifyOutput": false,
      "tdewolff": {
         "css": {
            "decimals": -1,
            "keepCSS2": true
         },
         "html": {
            "keepConditionalComments": true,
            "keepDefaultAttrVals": true,
            "keepDocumentTags": true,
            "keepEndTags": true,
            "keepWhitespace": false
         },
         "js": {},
         "json": {},
         "svg": {
            "decimals": -1
         },
         "xml": {
            "keepWhitespace": false
         }
      }
   },
    "enableEmoji":"true",
    "enableRobotsTXT":"true",
    "rssLimit":10,
    "relativeurls":"true",
    "canonifyURLs":"true",
